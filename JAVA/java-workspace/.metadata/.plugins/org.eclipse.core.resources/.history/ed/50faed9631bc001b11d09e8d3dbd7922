package kr.ac.kopo.week4.day16;

/*
 * alt + shift + s 누른 후 선택
 * c : 기본 생성자
 * o : 매개변수를 가지는 생성자
 */

public class UserInfo {

	public UserInfo() {
		super();
		// TODO Auto-generated constructor stub
	}
	public UserInfo(String name, int age, String addr) {
		super();
		this.name = name;
		this.age = age;
		this.addr = addr;
	}
	private String name;
	private int age;
	private String addr;

	
	
	
}
