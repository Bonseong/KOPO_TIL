		list.add(user3);
		
		FileInputStream fis = null;
		
		try {
			fis = new FileInputStream("userList.txt");
			ObjectInputStream ois = new ObjectInputStream(fis);
			
			
		} catch (Exception e) {
			e.printStackTrace();
		} finally {

		}
	}

